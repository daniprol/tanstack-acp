{
  "name": "tanstack-acp",
  "version": "0.1.0",
  "description": "TanStack AI adapter for Agent Client Protocol (ACP). Connect your React apps to ACP-compatible AI agents over WebSocket.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --out-dir dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --out-dir dist",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.13.0"
  },
  "peerDependencies": {
    "@tanstack/ai": "^0.3.0",
    "@tanstack/ai-react": "^0.3.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@tanstack/ai": "^0.3.0",
    "@tanstack/ai-react": "^0.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "tanstack",
    "ai",
    "acp",
    "agent",
    "protocol",
    "websocket",
    "chat",
    "llm",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniprol/tanstack-acp.git"
  },
  "bugs": {
    "url": "https://github.com/daniprol/tanstack-acp/issues"
  },
  "homepage": "https://github.com/daniprol/tanstack-acp"
}
