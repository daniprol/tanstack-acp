{
  "name": "tanstack-acp-monorepo",
  "private": true,
  "version": "0.1.0",
  "description": "TanStack AI adapter for Agent Client Protocol (ACP). Connect your React apps to ACP-compatible AI agents over WebSocket.",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "demo": "pnpm --filter tanstack-acp-demo dev"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "tanstack",
    "ai",
    "acp",
    "agent",
    "protocol",
    "websocket",
    "chat"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniprol/tanstack-acp.git"
  },
  "bugs": {
    "url": "https://github.com/daniprol/tanstack-acp/issues"
  },
  "homepage": "https://github.com/daniprol/tanstack-acp"
}
